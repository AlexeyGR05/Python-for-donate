class Cat:
    belonging1 = 'home'
    belonging2 = 'homeless'
    color = ['black', 'wite', 'gray']
    voice = 'Meow'
    after_eating = voice * 3


    def cat_life(self, belonging1, belonging2, color, voice, after_eating ):
        self.belonging1 = belonging1
        self.belonging2 = belonging1
        self.color = color
        self.voice = voice
        self.after_eating = after_eating


my_cat = Cat()
print(my_cat.belonging1)
print(my_cat.belonging2)
print(my_cat.color)
print(my_cat.voice)
print(my_cat.after_eating)
